{
  "name": "cosmostation",
  "version": "0.5.3",
  "description": "chrome extension cosmos wallet",
  "scripts": {
    "postinstall": "git config --local core.hooksPath ./git-hooks",
    "prestart:chrome": "rimraf build/chrome/dev",
    "start:chrome": "webpack --config webpack/webpack.dev.chrome.js --watch",
    "prestart:firefox": "rimraf build/firefox/dev",
    "start:firefox": "webpack --config webpack/webpack.dev.firefox.js --watch",
    "prebuild:chrome": "rimraf build/chrome/prod",
    "build:chrome": "webpack --config webpack/webpack.prod.chrome.js",
    "postbuild:chrome": "mkdir -p build/chrome/zip && cd build/chrome/prod && zip -r ../zip/extension-$(date +'%Y%m%dT%H%M%S').zip .",
    "prebuild:firefox": "rimraf build/firefox/prod",
    "build:firefox": "webpack --config webpack/webpack.prod.firefox.js",
    "postbuild:firefox": "mkdir -p build/firefox/zip && cd build/firefox/prod && zip -r ../zip/extension-$(date +'%Y%m%dT%H%M%S').zip .",
    "i18next-scanner": "i18next-scanner",
    "eslint": "eslint --max-warnings 0 $(git diff --name-only --diff-filter=AMRC HEAD | grep -E '.(ts|tsx)$' | grep -vE '.d.ts$' | xargs)",
    "patch": "yarn version --patch --no-git-tag-version",
    "postpatch": "yarn run update:version",
    "minor": "yarn version --minor --no-git-tag-version",
    "postminor": "yarn run update:version",
    "major": "yarn version --major --no-git-tag-version",
    "postmajor": "yarn run update:version",
    "update:manifest:chrome": "sed -i \"\" -e \"/\\\"version\\\":/s/:.*/: $(cat package.json | jq '.version'),/\" chrome/manifest.json",
    "update:manifest:firefox": "sed -i \"\" -e \"/\\\"version\\\":/s/:.*/: $(cat package.json | jq '.version'),/\" firefox/manifest.json",
    "update:env:chrome": "sed -i \"\" -e \"/^VERSION=/s/=.*/=$(cat package.json | jq '.version' | sed 's/\"//g')/\" .env.chrome",
    "update:env:firefox": "sed -i \"\" -e \"/^VERSION=/s/=.*/=$(cat package.json | jq '.version' | sed 's/\"//g')/\" .env.firefox",
    "update:env:chrome:dev": "sed -i \"\" -e \"/^VERSION=/s/=.*/=$(cat package.json | jq '.version' | sed 's/\"//g')/\" .env.chrome.development",
    "update:env:firefox:dev": "sed -i \"\" -e \"/^VERSION=/s/=.*/=$(cat package.json | jq '.version' | sed 's/\"//g')/\" .env.firefox.development",
    "update:version": "yarn run update:manifest:chrome && yarn run update:manifest:firefox && yarn run update:env:chrome && yarn run update:env:firefox && yarn run update:env:chrome:dev && yarn run update:env:firefox:dev"
  },
